@model WebBanThuoc.Data.ViewModels.PhanLoaiThuoc.PhanLoaiThuocVM

@{
    ViewData["Title"] = "AddPLT";
}


<div class="row" id="addplt">
    <div class="col-md-4">
        <form asp-action="AddPLT" id="formadd">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <input asp-for="Id" class="form-control id" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LoaiThuoc" class="control-label"></label>
                <input asp-for="LoaiThuoc" class="form-control" />
                <span asp-validation-for="LoaiThuoc" class="text-danger"></span>
            </div>
        </form>
    </div>
</div>



