@model IEnumerable<WebBanThuoc.DTOs.NhaCungCap>


@foreach (var item in Model) {
<tr>
	<td>
		@Html.DisplayFor(modelItem => item.Id)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.TenNhaCungCap)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.DiaChi)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.SDT)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.Gmail)
	</td>
	<td>
		<button class="btn btn-secondary btn-edit" data-id="@item.Id" data-toggle="modal" data-target="#exampleModal">
			<i class="fa-solid fa-pen-to-square"></i>
			Sửa
		</button>
		<button id="delete" class="btn btn-danger js-delete" data-id="@item.Id">
			<i class="fa fa-trash" aria-hidden="true"></i>
		</button>
	</td>
</tr>
}

