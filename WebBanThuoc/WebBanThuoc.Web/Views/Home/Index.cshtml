
@{
    ViewData["Title"] = "Home Page";
}

<section id="team" data-stellar-background-ratio="1">
	<div class="container">
		<div class="row">

			<div class="col-md-6 col-sm-6">
				<div class="about-info">
					<h2 class="wow fadeInUp" data-wow-delay="0.1s">Danh sách thuốc</h2>
				</div>
			</div>
			<div class="col-md-6 col-sm-6">
				<div class="about-info">
					<input class="form-control search" />
					<a class="btn btn-success js-timkiem" style="margin-top:10px;margin-left:465px">Tìm kiếm</a>
				</div>
			</div>
			<div class="clearfix"></div>

			<div class="col-md-4 col-sm-6" hidden>
				<div class="team-thumb wow fadeInUp" data-wow-delay="0.2s">
					<img src="images/team-image1.jpg" class="img-responsive" alt="">

					<div class="team-info">
						<h3>Nate Baston</h3>
						<p>General Principal</p>
						<div class="team-contact-info">
							<p><i class="fa fa-phone"></i> 010-020-0120</p>
							<p><i class="fa fa-envelope-o"></i> <a href="#">general@company.com</a></p>
						</div>
						<ul class="social-icon">
							<li><a href="#" class="fa fa-linkedin-square"></a></li>
							<li><a href="#" class="fa fa-envelope-o"></a></li>
						</ul>
					</div>

				</div>
			</div>


			<div id="list">
				<partial name="List" />
			</div>
		</div>
			<partial name="_Pager" />
	</div>
</section>
@section Scripts{ 
	<script src="~/client/js/list/showthuoc.js" asp-append-version="true"></script>
}